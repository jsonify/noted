    "configuration": {
      "title": "Noted",
      "properties": {
        "noted.notesFolder": {
          "type": "string",
          "default": "Notes",
          "description": "Folder name for storing notes"
        },
        "noted.fileFormat": {
          "type": "string",
          "enum": [
            "txt",
            "md"
          ],
          "default": "md",
          "description": "File format for notes"
        },
        "noted.useTemplate": {
          "type": "boolean",
          "default": false,
          "description": "Use a custom template for new notes"
        },
        "noted.template": {
          "type": "string",
          "default": "{date}\n==================================================\n\nPROBLEM:\n\n\nSTEPS TAKEN:\n1. \n\nSOLUTION:\n\n\nNOTES:\n",
          "description": "Template for new notes. Use {date} and {time} as placeholders.",
          "editPresentation": "multilineText"
        },
        "noted.tagAutoComplete": {
          "type": "boolean",
          "default": true,
          "description": "Enable tag autocomplete suggestions when typing # in metadata section of the file"
        },
        "noted.autoBacklinks": {
          "type": "boolean",
          "default": true,
          "description": "Automatically append backlinks section to notes when other notes link to them"
        },
        "noted.diagramsFolder": {
          "type": "string",
          "default": "Diagrams",
          "description": "Folder name for storing diagrams (Draw.io and Excalidraw files)"
        },
        "noted.graph.style.fontSize": {
          "type": "number",
          "default": 11,
          "description": "Font size for node labels in the graph view (constant pixel size, doesn't scale with zoom)"
        },
        "noted.graph.style.node.note": {
          "type": "string",
          "default": "#69db7c",
          "description": "Color for note nodes in the graph"
        },
        "noted.graph.style.node.tag": {
          "type": "string",
          "default": "#ff922b",
          "description": "Color for tag nodes in the graph"
        },
        "noted.graph.style.node.placeholder": {
          "type": "string",
          "default": "#e599f7",
          "description": "Color for placeholder nodes (unresolved links) in the graph"
        },
        "noted.graph.style.node.orphan": {
          "type": "string",
          "default": "#adb5bd",
          "description": "Color for orphan nodes (notes with no connections) in the graph"
        },
        "noted.graph.titleMaxLength": {
          "type": "number",
          "default": 24,
          "description": "Maximum length for node titles in the graph view"
        },
        "noted.graph.physics.chargeStrength": {
          "type": "number",
          "default": -120,
          "description": "Node repulsion strength (more negative = more spacing). Adaptive: -180 for dense graphs (50+ nodes)"
        },
        "noted.graph.physics.linkDistance": {
          "type": "number",
          "default": 50,
          "description": "Target distance between connected nodes. Adaptive: 70 for dense graphs (50+ nodes)"
        },
        "noted.graph.physics.collisionPadding": {
          "type": "number",
          "default": 1.5,
          "description": "Collision padding multiplier for node spacing. Adaptive: 2.5 for dense graphs (50+ nodes)"
        },
        "noted.ai.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI-powered summarization features"
        },
        "noted.ai.preferredModel": {
          "type": "string",
          "default": "",
          "description": "Preferred AI model ID for all AI features (tagging, summarization, search, templates). Empty = automatic selection. Use 'Noted: Select AI Model' command to choose from available models."
        },
        "noted.ai.summaryLength": {
          "type": "string",
          "enum": [
            "short",
            "medium",
            "long"
          ],
          "default": "medium",
          "description": "Default summary length"
        },
        "noted.ai.summaryFormat": {
          "type": "string",
          "enum": [
            "paragraph",
            "bullets",
            "structured"
          ],
          "default": "structured",
          "description": "Default summary format"
        },
        "noted.ai.includeActionItems": {
          "type": "boolean",
          "default": true,
          "description": "Extract action items in summaries"
        },
        "noted.ai.includeKeywords": {
          "type": "boolean",
          "default": false,
          "description": "Extract keywords/tags in summaries"
        },
        "noted.ai.cacheEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Cache generated summaries for better performance"
        },
        "noted.tagging.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic tagging features"
        },
        "noted.tagging.autoTagOnCreate": {
          "type": "boolean",
          "default": false,
          "description": "Automatically tag new notes when created"
        },
        "noted.tagging.maxTags": {
          "type": "number",
          "default": 5,
          "description": "Maximum number of tags to suggest per note",
          "minimum": 1,
          "maximum": 20
        },
        "noted.tagging.customCategories": {
          "type": "array",
          "default": [],
          "description": "Custom tag categories for your domain (e.g., project names, team names)",
          "items": {
            "type": "string"
          }
        },
        "noted.tagging.excludePatterns": {
          "type": "array",
          "default": [
            "draft",
            "temp"
          ],
          "description": "Tag patterns to exclude from suggestions",
          "items": {
            "type": "string"
          }
        },
        "noted.search.enableSemantic": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI-powered semantic search for natural language queries"
        },
        "noted.search.maxResults": {
          "type": "number",
          "default": 20,
          "description": "Maximum number of search results to display",
          "minimum": 1,
          "maximum": 100
        },
        "noted.search.minRelevanceScore": {
          "type": "number",
          "default": 0.5,
          "description": "Minimum relevance score (0.0-1.0) for search results",
          "minimum": 0,
          "maximum": 1
        },
        "noted.search.chunkSize": {
          "type": "number",
          "default": 3000,
          "description": "Maximum characters per note sent to AI for analysis",
          "minimum": 500,
          "maximum": 10000
        },
        "noted.search.maxCandidates": {
          "type": "number",
          "default": 20,
          "description": "Maximum number of notes to analyze with AI in semantic search",
          "minimum": 5,
          "maximum": 50
        },
        "noted.search.hybridCandidates": {
          "type": "number",
          "default": 20,
          "description": "Number of top keyword results to re-rank with AI in hybrid search",
          "minimum": 5,
          "maximum": 50
        },
        "noted.search.enableFuzzyMatch": {
          "type": "boolean",
          "default": false,
          "description": "Enable fuzzy matching to find results with typos"
        },
        "noted.templates.useAI": {
          "type": "boolean",
          "default": true,
          "description": "Use AI to enhance template creation and suggestions"
        },
        "noted.templates.defaultCategory": {
          "type": "string",
          "default": "Custom",
          "description": "Default category for new custom templates"
        },
        "noted.templates.enableBundles": {
          "type": "boolean",
          "default": true,
          "description": "Enable multi-note workflow bundles (Phase 2 feature)"
        },
        "noted.templates.showBrowser": {
          "type": "boolean",
          "default": false,
          "description": "Show template browser instead of quick pick (Phase 4 feature)"
        },
        "noted.templates.preferredModel": {
          "type": "string",
          "default": "",
          "description": "Preferred AI model ID for template generation (empty = automatic selection). Use 'Noted: Select AI Model' command to choose from available models."
        },
        "noted.templates.defaultAuthor": {
          "type": "string",
          "default": "",
          "description": "Default author name for generated templates (leave empty to use system username)"
        }
      }
    }
  },
