    "keybindings": [
      {
        "command": "noted.deleteNote",
        "key": "delete",
        "mac": "delete",
        "when": "focusedView == notedView || focusedView == notedTemplatesView || focusedView == notedJournalView || focusedView == notedConnectionsView || focusedView == notedOrphansView || focusedView == notedPlaceholdersView"
      },
      {
        "command": "noted.deleteNote",
        "key": "backspace",
        "mac": "backspace",
        "when": "focusedView == notedView || focusedView == notedTemplatesView || focusedView == notedJournalView || focusedView == notedConnectionsView || focusedView == notedOrphansView || focusedView == notedPlaceholdersView"
      },
      {
        "command": "noted.deleteCurrentNote",
        "key": "ctrl+shift+backspace",
        "mac": "cmd+shift+backspace",
        "when": "editorTextFocus"
      },
      {
        "command": "noted.deleteCurrentNote",
        "key": "ctrl+shift+delete",
        "mac": "cmd+shift+delete",
        "when": "editorTextFocus"
      }
    ],
    "submenus": [
      {
        "id": "noted.editorContext",
        "label": "Noted"
      }
    ],
    "menus": {
      "noted.editorContext": [
        {
          "command": "noted.extractSelectionToNote",
          "when": "editorHasSelection",
          "group": "1_edit@1"
        },
        {
          "command": "noted.summarizeCurrentNote",
          "when": "resourcePath =~ /Notes.*\\.(txt|md)$/",
          "group": "2_ai@1"
        },
        {
          "command": "noted.generateTagsCurrentNote",
          "when": "resourcePath =~ /Notes.*\\.(txt|md)$/",
          "group": "2_ai@2"
        }
      ],
      "editor/context": [
        {
          "submenu": "noted.editorContext",
          "when": "resourcePath =~ /Notes.*\\.(txt|md)$/ || editorHasSelection",
          "group": "1_noted@1"
        }
      ],
      "view/title": [
        {
          "command": "noted.createQuickNote",
          "when": "view == notedView",
          "group": "navigation@1"
        },
        {
          "command": "noted.openWithTemplate",
          "when": "view == notedView",
          "group": "navigation@2"
        },
        {
          "command": "noted.createFolder",
          "when": "view == notedView",
          "group": "navigation@3"
        },
        {
          "command": "noted.searchNotes",
          "when": "view == notedView",
          "group": "navigation@4"
        },
        {
          "command": "noted.quickSwitcher",
          "when": "view == notedView",
          "group": "navigation@4.5"
        },
        {
          "command": "noted.refresh",
          "when": "view == notedView",
          "group": "navigation@5"
        },
        {
          "command": "noted.showGraph",
          "when": "view == notedView",
          "group": "navigation@6"
        },
        {
          "command": "noted.showActivity",
          "when": "view == notedView",
