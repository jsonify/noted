{
  "name": "noted",
  "displayName": "Noted",
  "description": "Comprehensive workspace notes with wiki-style links, diagram embeds (Draw.io & Excalidraw), graph visualization, templates, advanced search, and daily tracking",
  "version": "1.43.10",
  "icon": "icon.png",
  "publisher": "jsonify",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonify/noted"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "notes",
    "note-taking",
    "daily notes",
    "journal",
    "productivity",
    "markdown",
    "templates",
    "tags",
    "wiki",
    "knowledge base",
    "diagrams",
    "drawio",
    "excalidraw",
    "graph",
    "embeds"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
